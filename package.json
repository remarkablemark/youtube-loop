    "eslint": "8.48.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
